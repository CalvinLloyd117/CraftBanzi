{% extends '_html5' %}

{% set carouselEntries = craft.entries().section('aboutCarouselEntry') %}
{% set elemCount = craft.entries().section('aboutCarouselEntry').count() %}

{% set labMemberEntries = craft.entries.section('labMemberProfile') %}
{# {% set pastLabMembers = craft.entries.section('pastLabMembers') %} #}
{# {% set currentCollaborators = craft.entries.section('currentCollaborators') %} #}

{% block body %}
  {{ parent() }}
  <div class="wrapper" id="wrapper">
    <!-- Slideshow container -->
    {% include 'modules/_carousel' with {
      entries: carouselEntries,
      count: elemCount
    } %}
    {# Lab Members Section #}
    {% include 'modules/_labMembersSection' with {
      labMembers: labMemberEntries
    } %}
    <!-- Distinguished Visiting Research Chair section -->
    {# {% include 'modules/_pastLabMembersSection' with {
      pastMembers: pastLabMembers
    } %} #}
    {{ sprig('modules/_pastLabMembersSection') }}
    {{ sprig.script }}
    <!-- Current Collaborators section -->
    {# {% include 'modules/_currentCollaboratorsSection' with {
      collaborators: currentCollaborators
    } %} #}
    {{ sprig('modules/_currentCollaboratorsSection') }}
    {{ sprig.script }}
    <!-- End Current Collaborators section -->
  </div>
{% endblock %}
